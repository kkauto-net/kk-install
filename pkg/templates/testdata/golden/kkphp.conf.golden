[www]
user = www-data
group = www-data
listen = /var/run/kkphp.sock
listen.owner = www-data
listen.group = www-data
listen.mode = 0660
clear_env = no

; # User Config
pm = dynamic
pm.max_children = 20
pm.start_servers = 4
pm.min_spare_servers = 4
pm.max_spare_servers = 20
pm.process_idle_timeout = 20s
request_terminate_timeout = 300

; Security
security.limit_extensions = .php
